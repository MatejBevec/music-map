{
  "hosting": {
    "predeploy": [
      "export FIREBASE_HOSTING_UPLOAD_CONCURRENCY=20"
    ],
    "public": "client",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "client/index.html"
      }
    ],
    "timeout": 150000
  }
}
